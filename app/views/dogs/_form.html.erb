  <%= simple_form_for(@dog) do |f| %>
  <%= f.input :name  %>
  <%= f.input :age  %>
  <%= f.input :breed  %>
  <%= f.input :size  %>
  <%= f.input :price  %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :photo  %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: "btn-primary" %>

  <% end %>
