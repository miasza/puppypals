<div id="search-form">
  <%= form_tag dogs_path, class: "form-inline", method: :get do %>
    <%= text_field_tag :date,
      params[:date],
      class: "form-control",
      placeholder: "Find a friend"
    %>
    <%= button_tag "find a friend", class: "form-control btn btn-light" %>
  <% end %>

</div>
